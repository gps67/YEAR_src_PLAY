
proc v_pack {item} { pack $item -side top -fill x }
proc v_pack_big {item} { pack $item -side top -fill x -expand y }
proc h_pack {item} { pack $item -side left -fill y }
proc h_pack_big {item} { pack $item -side left -fill both -expand y }
