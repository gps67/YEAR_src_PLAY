. ../go__fns
PATH=$prefix/bin:$PATH
tk_vern=tk9.0b3
tk_vern=tk9.0
tk_vern=tk9.0.0
url=http://prdownloads.sourceforge.net/tcl/${tk_vern}-src.tar.gz
fn_grp_url tcl $url
fn_fetch
fn_trim -src
ln_s ${tk_vern}-src.tar.gz $tools_2000_ftp/$grp/${tk_vern}.tar.gz
fn_unpack
# pick the ../unix/. subdir of src and of obj
fn_sub_src_sub_obj unix
fn_with_X11
fn_configure_pfx \
--enable-gcc \
--enable-threads \
--with-tcl=$prefix/lib/ \
#
fn_make 
fn_make install
