	$(RM_rf)  $(PROG3_EXE)
	$(RM_rf)  $(PROG4_EXE)
	$(RM_rf)  $(PROG5_EXE)
	$(RM_rf)  $(PROG6_EXE)
	$(RM_rf)  $(PROG7_EXE)
	$(RM_rf)  $(PROG8_EXE)
	$(RM_rf)  $(PROG9_EXE)
	$(RM_rf)  $(PROG0_EXE)
	$(RM_rf)  tv_vol_

