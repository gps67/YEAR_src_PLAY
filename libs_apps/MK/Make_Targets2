# link _INTO_ lib_x cant refer to X itself
# link _WITH_ lib_x does refer to X itself

LINKLIB11=`$(PKG_CONFIG) _into_base1 $(PKGS) --libs`
LINKLIB21=`$(PKG_CONFIG) _into_base2 libs_apps_base1 --libs`
