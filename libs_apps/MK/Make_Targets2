	$(RM_rf)  $(PROG2).o $(PROG2_EXE)
	$(RM_rf)  $(PROG3).o $(PROG3_EXE)
	$(RM_rf)  $(PROG4).o $(PROG4_EXE)
	$(RM_rf)  $(PROG5).o $(PROG5_EXE)
