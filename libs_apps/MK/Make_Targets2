	# $@
	#
	$(CXX) $(OPTIM) -o $@ $(PROG8).o $(OBJS) -L. $(WITHLIB21) -l$(LIB33) -l$(LIB35) -l$(LIB21) -l$(LIB11) $(LIBS)

