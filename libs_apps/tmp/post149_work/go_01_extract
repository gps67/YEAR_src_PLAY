# ls138
# post_README_gps.txt
# .env1 # VN=49
# .env1 # VN=52
# .env1 # tar=post-1.$VN.tar.gz
# .env1 # post=post-1$VN
# .env1 # work=work
# .env1 # # post-1.49.tar.gz
# .env1 # # post-1.52.tar.gz
# .env1 # 
. ./.env1

mkdir $work
cd $work

tar -zxvf ../$tar

mkdir $post

L1=" 
	readme.htm
	array.cxx
	array.h
	classinfo.cxx
	classinfo.h
	comptime.cxx
	dnmarr.h
	file.cxx
	file.h
	object.h
	stdtp.h
	storage.cxx
	storage.h
"

for f in $L1
do
	cp -p post/$f $post/post_$f
done
exit

